{
    "manifest_version": 2,
    "name": "TL",
    "version": "1.0",
    "description": "天龙八部畅易阁抢号",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts":[
        {
            "matches": ["http://tl.cyg.changyou.com/goods/*"],
            "js": ["jquery-3.4.1.js","popup.js"],
            "run_at":"document_start"
        }
    ],
    "permissions": [
        "tabs","http://*/*","https://*/*"
    ]
}